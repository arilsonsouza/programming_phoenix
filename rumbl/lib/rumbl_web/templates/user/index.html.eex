<h1 class="text-center">Listing Users</h1>

<table class="table table-hover table-bordered">
  <%= for user <- @users do %>
    <tr>
      <td>
        <%= render "user.html", user: user %>
      </td>
      <td>
        <%= link "View", to: Routes.user_path(@conn, :show, user.id) %>
      </td>
    </tr>
  <% end %>
</table>
